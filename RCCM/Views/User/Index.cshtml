@model List<UserViewModel>

<h1>User List</h1>

<table class="table">
    <thead>
        <tr>
            <th>User Name</th>
            <th>User Role</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.UserName</td>
                <td>@user.Role</td>
                
            </tr>
        }
    </tbody>
</table>
